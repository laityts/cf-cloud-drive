{
  "name": "cf-cloud-drive",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "preview": "npm run build && npx opennextjs-cloudflare build && npx wrangler dev",
    "deploy": "npm run build && npx opennextjs-cloudflare build && npx wrangler deploy",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply timgs --local",
    "db:migrate:prod": "wrangler d1 migrations apply timgs --remote"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.14.7",
    "aws4fetch": "^1.0.20",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.562.0",
    "next": "15.5.7",
    "next-intl": "^4.6.1",
    "react": "19.0.3",
    "react-dom": "19.0.3",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251231.0",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "bcryptjs": "^3.0.3",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "jose": "^6.1.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.54.0"
  },
  "overrides": {
    "next": "$next",
    "react": "$react",
    "react-dom": "$react-dom"
  }
}
